#include <stdio.h>
#include <string.h>
#include <vector>
#include <numeric>
#include <deque>
#include <iterator>
#include <algorithm>
#include <iostream>

int main() {
	const int MAX_SIZE = 10000;
	char line[MAX_SIZE];
	char num[MAX_SIZE];
	std::vector<std::vector<int> > vv;
	int maxlen = 0;

	while (fgets(line, MAX_SIZE, stdin)) {
		sscanf(line, "%s", num);

		if (strcmp(num, "0") == 0) {
			break;
		}

//		printf("B%sE\n", num);

		int len = strlen(num);

		if (len > maxlen) {
			int diff = len - maxlen;

			for (int i = 0; i < diff; ++i) {
				std::vector<int> v;
				vv.push_back(v);
			}

			maxlen = len;
		}

		int j = 0;

		for (int i = len - 1; i >= 0; --i) {
			char c = num[i];
			int d = c - 48;
			vv[j].push_back(d);
			++j;
		}
	}

	int r = 0;
	std::deque<int> deq;
	for (auto v : vv) {
		int sum = std::accumulate(v.begin(), v.end(), r);
		deq.push_front(sum % 10);
		r = sum / 10;
	}

	printf("%d", r);
	std::ostream_iterator<int> out_it(std::cout, "");
	std::copy(deq.begin(), deq.end(), out_it);
	std::cout << std::endl;

	return 0;
}
