#include <algorithm>
#include <stdio.h>
#include <string.h>

int main() {
	char code[50];

	while (scanf("%s", code) != EOF) {
		printf("B%sE\n", code);
	}

	return 0;

	int len = strlen(code);

	int i = 1;
	do {
		printf("%d: %s\n", i, code);
		++i;
	} while (std::next_permutation(code, code+len));

	return 0;
}
